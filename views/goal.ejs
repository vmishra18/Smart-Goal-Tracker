  <section >


    <div class="table-data">
<input id="myInput" type="text" placeholder="Search..">
<div class="table-responsive">

    <table  id="customers" class="table ws-table-all" style="background-color:#fff;color:#000">
      <thead>
        <tr>
            <th>S.N</th>
            <th>Operations</th>
            <th>Objective</th>
            <th>Owner</th>
            <th>Quarter</th>
            <th>Description</th>
            <th>Team</th>
            <th>Role</th>
            <th>Success_Metric</th>
            <th>Current_Result</th>
            <th>Current_Satus</th>
        </tr>

      </thead>

        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tbody  id="myTable">
          <tr>
              <td><%=i; %></td>
              <td><%=data.Operations %></td>
              <td><%=data.Objective %></td>
              <td><%=data.Owner %></td>
              <td><%=data.Quarter %></td>
              <td><%=data.Description %></td>
              <td><%=data.Team %></td>
              <td><%=data.Role %></td>
              <td><%=data.Success_Metric %></td>
              <td><%=data.Current_Result %></td>
              <td><%=data.Current_Satus %></td>

          </tr>
        </tbody>

        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
